<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="16dp">

    <TextView
        android:id="@+id/tv_category_board_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Board ID"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tv_favorite_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Favorite Title"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/tv_date_created"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Date Created"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tv_category_nick"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="Nickname" />

    <TextView
        android:id="@+id/tv_category_like_cnt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="Like Count" />

    <!-- 여기에 필요한 추가 텍스트 뷰를 더 추가할 수 있습니다 -->

</LinearLayout>
